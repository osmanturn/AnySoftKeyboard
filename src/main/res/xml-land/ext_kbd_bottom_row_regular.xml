    <Row
        android:keyHeight="@integer/key_normal_height"
        android:keyWidth="10%p"
        android:keyboardMode="@integer/keyboard_mode_url"
        android:rowEdgeFlags="bottom">
        <Key
            android:codes="-9"
            android:keyEdgeFlags="left"
            android:keyWidth="10%p"
            ask:isFunctional="true" />
        <Key
            android:codes="-3"
            android:keyWidth="10%p"
            ask:isFunctional="true" />

        <Key
            android:codes="63"
            android:keyLabel="\?"
            android:popupCharacters="!/@\u0026\u00bf\u00a1"
            ask:isFunctional="true" />
        <Key
            android:codes="58"
            android:keyLabel=":"
            ask:isFunctional="true" />

        <Key
            android:codes="32"
            android:keyWidth="25%p"
            ask:isFunctional="true" />

        <Key
            android:codes="46"
            android:keyLabel="."
            android:popupCharacters=",-_"
            ask:isFunctional="true" />
        <Key
            android:codes="47"
            android:keyLabel="/"
            ask:isFunctional="true" />

        <Key
            android:codes="10"
            android:keyEdgeFlags="right"
            android:keyWidth="15%p"
