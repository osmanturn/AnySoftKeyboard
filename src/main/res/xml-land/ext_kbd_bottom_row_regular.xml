<?xml version="1.0" encoding="utf-8"?>

<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ask="http://schemas.android.com/apk/res-auto">
    <!-- Generic bottom row -->
    <Row
        android:keyHeight="@integer/key_normal_height"
        android:keyWidth="10%p"
        android:keyboardMode="@integer/keyboard_mode_normal"
        android:rowEdgeFlags="bottom">
        <Key
            android:codes="-10"
            android:keyEdgeFlags="left"
            android:keyWidth="10%p"
            ask:isFunctional="true"
            ask:longPressCode="-102" />
        <Key
            android:codes="-3"
            android:keyWidth="10%p"
            ask:isFunctional="true" />

        <Key
            android:codes="63"
            android:keyLabel="\?"
            android:popupCharacters="!/@\u0026\u00bf\u00a1"
            ask:isFunctional="true" />
        <Key
            android:codes="44"
            android:keyLabel=","
            android:popupCharacters="()"
            ask:isFunctional="true" />

        <Key
            android:codes="32"
            android:keyWidth="25%p"
            ask:isFunctional="true" />

        <Key
            android:codes="46"
            android:keyLabel="."
            android:popupCharacters=";:-_\u00b7\u2026"
            ask:isFunctional="true" />
        <Key
            android:codes="39"
            android:keyLabel="\'"
            android:popupCharacters="&#34;\u201e\u201d"
            ask:isFunctional="true" />

        <Key
            android:codes="10"
            android:keyEdgeFlags="right"
            android:keyWidth="15%p"
            ask:longPressCode="-100" />
    </Row>
