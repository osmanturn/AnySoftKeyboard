        <Key
            android:codes="63"
            android:keyLabel="\?"
            android:popupCharacters="!/@\u0026\u00bf\u00a1"
            ask:isFunctional="true" />
        <Key
            android:codes="44"
            android:keyLabel=","
            android:popupCharacters="()"
            ask:isFunctional="true" />

        <Key
            android:codes="32"
            android:keyWidth="25%p"
            ask:isFunctional="true" />

        <Key
            android:codes="46"
            android:keyLabel="."
            android:popupCharacters=";:-_\u00b7\u2026"
            ask:isFunctional="true" />
        <Key
            android:codes="39"
            android:keyLabel="\'"
